{
  "name": "setup-config",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^4.0.2",
    "prettier": "^1.5.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "react-scripts": "1.0.10"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
        "prettier --single-quote --tab-width 4 --write",
        "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "pretty": "./node_modules/.bin/prettier --single-quote --tab-width 4 --write 'src/**/*.js'"
  }
}
